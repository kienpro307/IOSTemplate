# Appfile
# More information about the Appfile can be found at:
# https://docs.fastlane.tools/advanced/#appfile

# Apple ID / Email
apple_id(ENV['APPLE_ID'] || "your.email@example.com")

# Developer Portal Team ID
team_id(ENV['APPLE_TEAM_ID'] || "XXXXXXXXXX")

# App Store Connect Team ID (if different from Developer Portal)
itc_team_id(ENV['ITC_TEAM_ID'] || ENV['APPLE_TEAM_ID'] || "XXXXXXXXXX")

# App Bundle Identifier
app_identifier(ENV['APP_IDENTIFIER'] || "com.yourcompany.iostemplate")

